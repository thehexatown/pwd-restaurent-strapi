{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 30,
      "minLength": 10
    },
    "mediumPrice": {
      "type": "decimal",
      "default": 0,
      "required": true
    },
    "description": {
      "type": "text",
      "required": true,
      "maxLength": 100,
      "minLength": 15
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "smallPrice": {
      "type": "decimal",
      "default": 0,
      "required": true
    },
    "largePrice": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "extras": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::extra.extra"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::categoty.categoty"
    }
  }
}
